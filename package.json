{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "env-cmd -f .env gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.0",
    "gatsby-plugin-google-gtag": "^2.1.13",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-scroll-reveal": "0.0.7",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-source-wordpress-experimental": "^2.1.0",
    "gatsby-transformer-sharp": "^2.5.21",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.11.0",
    "swiper": "^6.3.5"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "gh-pages": "^3.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
